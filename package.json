{
  "name": "mmir-plugin-tts-nuance-xhr",
  "version": "1.0.0",
  "description": "Cordova plugin for the MMIR framework that adds Text To Speech (TTS) synthesis via Nuance web services",
  "main": "./www/ttsNuanceXhr.js",
  "directories": {
    "lib": "./www"
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "createplugincompat ./ && pluginexport ./",
    "update:version": "updateversion ./",
    "update:res": "copycordovascripts res/js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmig/mmir-plugin-tts-nuance-xhr.git"
  },
  "keywords": [
    "tts",
    "speech",
    "recognition",
    "nuance",
    "xhr"
  ],
  "author": "russa",
  "license": "MIT",
  "cordova": {
    "id": "mmir-plugin-tts-nuance-xhr",
    "platforms": []
  },
  "engines": {
    "cordovaDependencies": {
      "1.0.0": {
        "mmir-plugin-speech-nuance-lang": ">=1.1.0"
      }
    }
  },
  "mmir": {
    "compat": {
      "./www/ttsNuanceXhr.js": {
        "file": "./www/alt/ttsNuanceXhrCompat.js",
        "type": "tts"
      }
    }
  },
  "devDependencies": {
    "mmir-lib": "^6.2.0",
    "mmir-plugin-exports": "^2.4.0"
  },
  "dependencies": {
    "mmir-plugin-speech-nuance-lang": "^1.1.0"
  }
}
