{
  "name": "mmir-plugin-tts-nuance-xhr",
  "version": "1.0.0",
  "description": "Cordova plugin for the MMIR framework that adds Text To Speech (TTS) synthesis via Nuance web services",
  "main": "./www/webttsNuanceImpl.js",
  "directories": {
    "lib": "./www"
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "createplugincompat ./ && pluginexport ./"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-tts-nuance-xhr.git"
  },
  "keywords": [
    "tts",
    "speech",
    "recognition",
    "nuance",
    "xhr"
  ],
  "author": "russa",
  "license": "MIT",
  "peerDependencies": {
    "mmir-lib": "git+https://gitlab-cos.b.dfki.de/mmir/dev_mmir-lib.git#develop/webpack"
  },
  "cordova": {
    "id": "mmir-plugin-tts-nuance-xhr",
    "platforms": []
  },
  "engines": {
    "cordovaDependencies": {}
  },
  "mmir": {
    "compat": {
      "./www/webttsNuanceImpl.js": {
        "file": "./www/alt/webttsNuanceImplCompat.js",
        "type": "tts"
      }
    }
  },
  "devDependencies": {
    "mmir-plugin-exports": "git+https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-exports.git"
  }
}
